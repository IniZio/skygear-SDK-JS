{
  "presets": [
    "es2015",
    "react",
    "stage-2"
  ],
  "plugins": [
    "add-module-exports",
    ["transform-builtin-extend", {
      "globals": ["Error", "Array"],
      "approximate": true
    }],
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }]
  ]
}
